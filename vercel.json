{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/status", 
      "dest": "/api/status.js"
    },
    {
      "src": "/api/convert-3d",
      "dest": "/api/convert-3d.js"
    },
    {
      "src": "/api/download",
      "dest": "/api/download.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/status.js"
    }
  ]
}
