{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/server.js"
    },
    {
      "src": "/api/status",
      "dest": "/server.js"
    },
    {
      "src": "/api/convert-3d",
      "dest": "/server.js"
    },
    {
      "src": "/api/download",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
